#!/usr/bin/env mkb
# Builder mkb file for the java portion of the gamepad extension on android
platform JAVA

files
{
    (source/android)
    gamepad.java
    gamepadActivity.java
    GamepadInfo.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/gamepad.jar
